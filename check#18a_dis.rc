typedef funcptr : int (int x, int y) TWOINTFUNC;
TWOINTFUNC ptr1, ptr2;

function : int addition(int x, int y) {return x + y;}
function : int subtraction(int x, int y) {return x - y;}

function : int main(){
  if(ptr1 == nullptr){ 
    ptr1 = addition;
  }
  ptr1(4, 6);
  ptr2 = subtraction;
  ptr2(5,2);
  ptr2 = ptr1;
  ptr2(5, 2);
  ptr2 = nullptr;
  
  return 0;
}